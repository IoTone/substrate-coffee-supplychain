  "dependencies": {
    "@babel/helper-define-map": "^7.16.0",
    "@babel/helper-regex": "^7.10.5",
    "@pathofdev/react-tag-input": "^1.0.7",
    "@polkadot/api": "^6.9.2",
    "@polkadot/extension-dapp": "^0.41.1",
    "@polkadot/types": "^6.9.2",
    "@polkadot/ui-keyring": "^0.86.7",
    "@polkadot/ui-settings": "^0.86.7",
    "@polkadot/util": "^7.9.2",
    "@polkadot/util-crypto": "^7.9.2",
    "invariant": "^2.2.4",
    "levenary": "^1.1.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.2",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-tooltip": "^4.2.21",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^1.2.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^3.2.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src/**",
    "lint:ci": "eslint src/** --max-warnings=0",
    "lint:fix": "eslint --fix src/**",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build -m '[ci skip] Updates'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "standard"
    ],
    "plugins": [
      "only-warn"
    ],
    "rules": {
      "semi": [
        1,
        "always"
      ],
      "no-extra-semi": 1
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://substrate-developer-hub.github.io/substrate-front-end-template",
  "bugs": {
    "url": "https://github.com/substrate-developer-hub/substrate-front-end-template/issues"
  },
  "keywords": [
    "substrate",
    "substrate-ui",
    "polkadot-js"
  ]
}
